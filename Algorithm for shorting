#include<stdio.h>
#include<conio.h>
void Swap(int *p1, int *p2)
{
	int temp = *p1;
	*p1= *p2;
	*p2 = temp;
}

void Shorting(int a[], int n)
{
	int i, j;
	for(i=0; i<n; i++)
	{
		for(j=i+1; j<n; j++)
		{
			if(a[i]>a[j])
			{
				Swap(&a[i], &a[j]);
			}
		}
	}
}

void Print(int a[], int n)
{
	int i;
	for(i=0; i<n; i++)
	{
		printf(" %d",a[i]);
	}
}


int main()
{
	int i,n;
	printf("enter the value");
	scanf("%d",&n);
	int a[n];
		for(i=0; i<n; i++)
		{
			scanf("%d",&a[i]);
		}
		Shorting(a, n);
		Print(a,n);
		return 0;
}
